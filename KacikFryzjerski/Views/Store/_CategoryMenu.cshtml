@model IEnumerable<KacikFryzjerski.Models.CategoryModels>

<nav>
    <ul>
        @foreach (var storeCategory in Model)
        {
            <li>
                <a class="store-category" href="@Url.Action("CategoryList", "Store", new { category_name = storeCategory.Category_name.ToString() })">@storeCategory.Category_name.ToString()</a>
            </li>
        }
    </ul>
</nav>